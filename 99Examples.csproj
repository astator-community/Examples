<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0-android</TargetFramework>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="astator.Core" Version="*" />
        <PackageReference Include="Serilog" Version="2.10.0" />
        <PackageReference Include="Xamarin.AndroidX.AppCompat" Version="*" />
    </ItemGroup>

    <ProjectExtensions>
        <IsObfuscate>true</IsObfuscate>
        <UseOCR>true</UseOCR>
        <ApkBuilderConfigs>
            <Label>Examples</Label>
            <PackageName>com.script.Examples</PackageName>
            <Version>0.3.0</Version>
        </ApkBuilderConfigs>
    </ProjectExtensions>

</Project>
 